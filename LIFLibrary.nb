Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"\[Alpha]", "[", 
    RowBox[{"k_", ",", "Xr_", ",", "Xt_"}], "]"}], ":=", 
   SuperscriptBox[
    RowBox[{"(", 
     FractionBox[
      RowBox[{"2", "\[Pi]", " ", "k"}], 
      RowBox[{"Xt", "-", "Xr"}]], ")"}], "2"]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"\[Beta]", "[", 
    RowBox[{"Xr_", ",", "Xt_"}], "]"}], ":=", 
   SuperscriptBox[
    RowBox[{"(", 
     FractionBox[
      RowBox[{"Xt", "+", "Xr"}], "2"], ")"}], "2"]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"\[Gamma]", "[", 
    RowBox[{"k_", ",", "Xr_", ",", "Xt_"}], "]"}], ":=", 
   FractionBox[
    RowBox[{
     RowBox[{
      RowBox[{"-", "4"}], " ", 
      SuperscriptBox["k", "2"], " ", 
      SuperscriptBox["\[Pi]", "2"]}], "+", 
     RowBox[{"2", " ", "Xr", " ", 
      RowBox[{"(", 
       RowBox[{"Xr", "-", "Xt"}], ")"}], " ", 
      RowBox[{"Log", "[", 
       RowBox[{"1", "+", 
        SqrtBox[
         RowBox[{"1", "-", 
          SuperscriptBox["\[ExponentialE]", 
           RowBox[{
            RowBox[{"-", "2"}], " ", "Xr", " ", 
            RowBox[{"(", 
             RowBox[{"Xr", "-", "Xt"}], ")"}]}]]}]]}], "]"}]}], "+", 
     SuperscriptBox[
      RowBox[{"Log", "[", 
       RowBox[{"1", "+", 
        SqrtBox[
         RowBox[{"1", "-", 
          SuperscriptBox["\[ExponentialE]", 
           RowBox[{
            RowBox[{"-", "2"}], " ", "Xr", " ", 
            RowBox[{"(", 
             RowBox[{"Xr", "-", "Xt"}], ")"}]}]]}]]}], "]"}], "2"]}], 
    RowBox[{"2", 
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"Xr", "-", "Xt"}], ")"}], "2"], " "}]]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"LambdaDDGuess", "[", 
    RowBox[{"k_", ",", "Xr_", ",", "Xt_"}], "]"}], ":=", 
   RowBox[{"If", "[", 
    RowBox[{
     RowBox[{"Xt", "\[Equal]", 
