Cell[BoxData[
 RowBox[{
  RowBox[{"GetLambdasFromGuess", "[", 
   RowBox[{"Xr_", ",", "Xt_", ",", "\[Lambda]\[Tau]0_"}], "]"}], ":=", 
  "\[IndentingNewLine]", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"\[Lambda]\[Tau]", ",", "\[Lambda]\[Tau]1", ",", "ndx", ",", 
      RowBox[{"NumericalZero", "=", 
       SuperscriptBox["10", 
        RowBox[{"-", "10"}]]}]}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"Depth", "[", "\[Lambda]\[Tau]0", "]"}], "\[Equal]", "1"}], 
       ",", 
       RowBox[{"\[Lambda]\[Tau]1", "=", 
        RowBox[{"{", "\[Lambda]\[Tau]0", "}"}]}], ",", 
       RowBox[{"\[Lambda]\[Tau]1", "=", "\[Lambda]\[Tau]0"}]}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"\[Lambda]\[Tau]", "=", 
      RowBox[{"Table", "[", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"Im", "[", 
            RowBox[{"\[Lambda]\[Tau]1", "[", 
             RowBox[{"[", "n", "]"}], "]"}], "]"}], "\[Equal]", "0"}], ",", 
          "\[IndentingNewLine]", 
          RowBox[{"If", "[", 
           RowBox[{
            RowBox[{"Xt", "\[Equal]", 
             RowBox[{"-", "Xr"}]}], ",", "\[IndentingNewLine]", 
            RowBox[{"\[Lambda]\[Tau]", "/.", 
             RowBox[{"FindRoot", "[", 
              RowBox[{
               RowBox[{
                RowBox[{"CE", "[", 
                 RowBox[{"\[Lambda]\[Tau]", ",", "Xr", ",", "Xt"}], "]"}], 
                "\[Equal]", "0"}], ",", 
               RowBox[{"{", 
                RowBox[{"\[Lambda]\[Tau]", ",", 
                 RowBox[{"Re", "[", 
                  RowBox[{"\[Lambda]\[Tau]1", "[", 
                   RowBox[{"[", "n", "]"}], "]"}], "]"}]}], "}"}]}], "]"}]}], 
            ",", "\[IndentingNewLine]", 
            RowBox[{"\[Lambda]\[Tau]", "/.", 
             RowBox[{"FindRoot", "[", 
              RowBox[{
               RowBox[{
                RowBox[{"CEDD", "[", 
                 RowBox[{"\[Lambda]\[Tau]", ",", "Xr", ",", "Xt"}], "]"}], 
                "\[Equal]", "0"}], ",", 
               RowBox[{"{", 
                RowBox[{"\[Lambda]\[Tau]", ",", 
                 RowBox[{"Re", "[", 
                  RowBox[{"\[Lambda]\[Tau]1", "[", 
                   RowBox[{"[", "n", "]"}], "]"}], "]"}]}], "}"}]}], 
              "]"}]}]}], "\[IndentingNewLine]", "]"}], ",", 
          "\[IndentingNewLine]", 
          RowBox[{"\[Lambda]\[Tau]", "/.", 
           RowBox[{"FindRoot", "[", 
            RowBox[{
             RowBox[{
              RowBox[{"CED", "[", 
               RowBox[{"\[Lambda]\[Tau]", ",", "Xr", ",", "Xt"}], "]"}], 
              "\[Equal]", "0"}], ",", 
             RowBox[{"{", 
              RowBox[{"\[Lambda]\[Tau]", ",", 
               RowBox[{"\[Lambda]\[Tau]1", "[", 
                RowBox[{"[", "n", "]"}], "]"}]}], "}"}]}], "]"}]}]}], 
         "\[IndentingNewLine]", "]"}], ",", "\[IndentingNewLine]", 
        RowBox[{"{", 
         RowBox[{"n", ",", 
          RowBox[{"Length", "[", "\[Lambda]\[Tau]1", "]"}]}], "}"}]}], 
       "]"}]}], ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"ndx", "=", 
      RowBox[{"Flatten", "[", 
       RowBox[{"Position", "[", 
        RowBox[{"\[Lambda]\[Tau]", ",", 
         RowBox[{"x_", "/;", 
          RowBox[{
           RowBox[{"Re", "[", "x", "]"}], ">", "0"}]}]}], "]"}], "]"}]}], ";",
      "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"Length", "[", "ndx", "]"}], ">", "0"}], ",", 
       RowBox[{"Print", "[", 
        RowBox[{"\"\<Warning: \>\"", ",", " ", 
         RowBox[{"Length", "[", "ndx", "]"}], ",", 
         "\"\< eigenvalues result to have Re[\[Lambda]]>0.\>\""}], "]"}]}], 
      "]"}], ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"ndx", "=", 
      RowBox[{"Flatten", "[", 
       RowBox[{"Position", "[", 
        RowBox[{
         RowBox[{"Table", "[", "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{"Min", "[", 
            RowBox[{
             RowBox[{"Abs", "[", 
              RowBox[{"CE", "[", 
               RowBox[{
                RowBox[{"\[Lambda]\[Tau]", "[", 
                 RowBox[{"[", "n", "]"}], "]"}], ",", "Xr", ",", "Xt"}], 
               "]"}], "]"}], ",", 
             RowBox[{"Abs", "[", 
              RowBox[{"CED", "[", 
               RowBox[{
                RowBox[{"\[Lambda]\[Tau]", "[", 
                 RowBox[{"[", "n", "]"}], "]"}], ",", "Xr", ",", "Xt"}], 
               "]"}], "]"}], ",", 
             RowBox[{"Abs", "[", 
              RowBox[{"CEDD", "[", 
               RowBox[{
                RowBox[{"\[Lambda]\[Tau]", "[", 
                 RowBox[{"[", "n", "]"}], "]"}], ",", "Xr", ",", "Xt"}], 
               "]"}], "]"}]}], "]"}], ",", "\[IndentingNewLine]", 
           RowBox[{"{", 
            RowBox[{"n", ",", 
             RowBox[{"Length", "[", "\[Lambda]\[Tau]", "]"}]}], "}"}]}], 
          "]"}], ",", 
         RowBox[{"x_", "/;", 
          RowBox[{"x", ">", "NumericalZero"}]}]}], "]"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"Length", "[", "ndx", "]"}], ">", "0"}], ",", 
       RowBox[{"Print", "[", 
        RowBox[{"\"\<Warning: \>\"", ",", " ", 
         RowBox[{"Length", "[", "ndx", "]"}], ",", 
         "\"\< eigenvalues do not satisfy CE[\[Lambda]]==0 \>\"", ",", "ndx", 
         ",", "\"\<.\>\""}], "]"}]}], "]"}], ";", "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", " ", 
      RowBox[{"\[Lambda]\[Tau]", "=", 
       RowBox[{"SortBy", "[", 
        RowBox[{"\[Lambda]\[Tau]", ",", 
         RowBox[{
          RowBox[{"-", 
           RowBox[{"Re", "[", "#", "]"}]}], "&"}]}], "]"}]}], " ", "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"Length", "[", "\[Lambda]\[Tau]", "]"}], "\[Equal]", "1"}], 
       ",", 
       RowBox[{"\[Lambda]\[Tau]", "=", 
        RowBox[{"\[Lambda]\[Tau]", "[", 
         RowBox[{"[", "1", "]"}], "]"}]}]}], "]"}], ";", 
     "\[IndentingNewLine]", "\[Lambda]\[Tau]"}]}], "\[IndentingNewLine]", 
   "]"}]}]], "Input",
 InitializationCell->True,
 CellChangeTimes->{{3.7555218147198095`*^9, 3.7555218757076173`*^9}, {
